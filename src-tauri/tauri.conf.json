{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Saldo",
  "version": "0.1.0",
  "identifier": "com.saldo.app",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://localhost:5000",
    "beforeDevCommand": "npm run dev:web",
    "beforeBuildCommand": "npm run build:web"
  },
  "app": {
    "windows": [
      {
        "title": "Saldo",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false
  }
}
